<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="CustomizeHandlers.Views.MainPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:CustomizeHandlers.Views">
    <TableView Intent="Menu">
        <TableRoot>
            <TableSection Title="Customized Handlers">
                <TextCell
                    Command="{Binding NavigateCommand}"
                    CommandParameter="{x:Type views:PickerView}"
                    Detail="Colorize item rows"
                    Text="Customimze a Picker" />
                <TextCell
                    Command="{Binding NavigateCommand}"
                    CommandParameter="{x:Type views:SearcBarView}"
                    Detail="Colorize Search icon"
                    Text="Customimze a SearcBar" />
            </TableSection>
        </TableRoot>
    </TableView>
</ContentPage>